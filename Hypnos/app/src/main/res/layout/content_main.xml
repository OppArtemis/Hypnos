<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.artemis.hypnos.android.MainActivity2"
    tools:showIn="@layout/activity_main">

    <TextView
        android:id="@+id/txtBabyName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="30dp"
        android:text="txtBabyName" />

    <TextView
        android:id="@+id/txtBabyDob"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="txtBabyDob"
        android:layout_below="@+id/txtBabyName"
        android:layout_alignLeft="@+id/txtBabyName"
        android:layout_alignStart="@+id/txtBabyName"
        android:layout_marginTop="15dp" />

    <TextView
        android:id="@+id/txtNewDayTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="txtNewDayTime"
        android:layout_below="@+id/txtBabyDob"
        android:layout_alignLeft="@+id/txtBabyDob"
        android:layout_alignStart="@+id/txtBabyDob"
        android:layout_marginTop="15dp" />




    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/txtNewDayTime"
        android:layout_marginLeft="150dp"
        android:text="Last Time" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="300dp"
        android:layout_alignBaseline="@+id/textView12"
        android:text="Total Today" />




    <Button
        android:id="@+id/btnPeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Peed"
        android:layout_below="@+id/txtNewDayTime"
        android:layout_alignLeft="@+id/txtNewDayTime"
        android:layout_alignStart="@+id/txtNewDayTime"
        android:layout_marginLeft="15dp" />

    <TextView
        android:id="@+id/txtTodayPeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="txtTodayPeed"
        android:layout_marginLeft="150dp"
        android:layout_alignBaseline="@+id/btnPeed"
        android:layout_alignBottom="@+id/btnPeed" />

    <TextView
        android:id="@+id/txtLastPeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="txtLastPeed"
        android:layout_alignBaseline="@+id/txtTodayPeed"
        android:layout_marginLeft="300dp" />



    <Button
        android:id="@+id/btnPooped"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Poop"
        android:layout_below="@+id/btnPeed"
        android:layout_alignLeft="@+id/btnPeed"
        android:layout_alignStart="@+id/btnPeed"
        android:layout_marginTop="15dp" />

    <TextView
        android:id="@+id/txtLastPooped"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btnPooped"
        android:layout_alignBottom="@+id/btnPooped"
        android:text="txtLastPooped"
        android:layout_marginLeft="150dp"/>

    <TextView
        android:id="@+id/txtTodayPooped"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="txtTodayPooped"
        android:layout_alignBaseline="@+id/txtLastPooped"
        android:layout_alignBottom="@+id/txtLastPooped"
        android:layout_marginLeft="300dp" />









